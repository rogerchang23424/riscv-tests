#=======================================================================
# Makefrag for rv32uzbs tests
#-----------------------------------------------------------------------

rv32uzbs_sc_tests = \
	bclr bclri \
	bext bexti \
	binv binvi \
	bset bseti \

rv32uzbs_p_tests = $(addprefix rv32uzbs-p-, $(rv32uzbs_sc_tests))
rv32uzbs_v_tests = $(addprefix rv32uzbs-v-, $(rv32uzbs_sc_tests))
